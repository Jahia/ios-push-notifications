[consequence][]Send iOS notification about {node} with category {category} title {title} and body {body}=notificationRuleService.sendNotification({node}, {category}, {title}, {body}, drools);
[consequence][]Send iOS notification to all users about {node} with category {category} title {title} and body {body}=notificationRuleService.sendNotificationToAll({node}, {category}, {title}, {body}, drools);
[consequence][]Send iOS notification to all candidates about {node} with category {category} title {title} and body {body}=notificationRuleService.sendNotificationToTaskCandidates({node}, {category}, {title}, {body}, drools);
