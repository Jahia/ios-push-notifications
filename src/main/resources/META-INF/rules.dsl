[consequence][]Send iOS notification about {node} with category {category} title {title} and body {body}=ruleService.sendNotification({node}, {category}, {title}, {body}, drools);
[consequence][]Send iOS notification to all users about {node} with category {category} title {title} and body {body}=ruleService.sendNotificationToAll({node}, {category}, {title}, {body}, drools);
